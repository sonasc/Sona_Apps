#fintech-merchants
  .root
    .container.fixed-width--md-up
      +title-main($t.fintech.title_partner, 'center')
      .tab
        .row.spacing6.justify-content-center
          .col-md-10.col-sm-12(class="md:mt-5")
            .d-flex.justify-content-center
              ul.tabs.tabs-slider.primary
                li.tab
                  |
                  |
                  a.tab-label.waves-effect.active(href='#tab-1') Online
                  |
                  |
                li.tab
                  |
                  |
