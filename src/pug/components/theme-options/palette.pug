  .theme-switched
    .card.paper
      .row
        .col-sm-6.margin-bottom
          h6.title Theme Mode
          .switch.switch-toggle
            label.overline
              | Light
              input#theme_switcher_side(type='checkbox')
              span.lever
              | Dark
        .col-sm-6
          h6.title Layout Direction
          .switch.switch-toggle
            label.overline
              | LTR
              input#dir_switcher_side(type='checkbox')
              span.lever
              | RTL
              br/
              i.caption Best view in arabic
  .image-switched
    .card.paper
      .row.mb-0
        .col-sm-6.margin-bottom
          h6.title Theme Illustration
        .col-sm-6
          .switch.switch-toggle
            label.overline
              | 2D
              input#image_switcher_side(type='checkbox' checked)
              span.lever
              | 3D
  .theme-color
    .card.paper
      h6.title Color Combination
      .row
        each clr, index in colorList
          .col-sm-3.col-6.my-1
            .swatch.waves-effect.tooltipped(
              data-position="top"
              data-tooltip=clr
              data-color=clr
            )
              span.primary-swatch(style='background: '+themeList[clr].primary)
                | &nbsp;
              span.secondary-swatch(style='background: '+themeList[clr].secondary)
                | &nbsp;
              span.accent-swatch(style='background: '+themeList[clr].accent)
                | &nbsp;